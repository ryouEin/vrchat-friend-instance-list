{
  "manifest_version": 3,
  "name": "VRChat Friend Instance List",
  "description": "Tool for VRChat. Show instances exist your friends.",
  "version": "4.2.2",
  "permissions": ["tabs"],
  "host_permissions": ["https://vrchat.com/", "https://www.vrchat.com/"],
  "action": {
    "default_icon": {
      "19": "icon_small.png",
      "38": "icon_big.png"
    }
  },
  "background": {
    "service_worker": "icon_clicked.js"
  }
}
